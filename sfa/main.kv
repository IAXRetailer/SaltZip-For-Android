#:kivy 2.1.0
<WarningPopup@Popup>:
    title: 'Warning'
    size_hint: None, None
    size: 400, 200
    BoxLayout:
        orientation: "vertical"
        Label:
            size_hint: None, None
            size: self.texture_size
            font_name:"Han_Font"
            text: '你确定选择这个文件吗\n选择的必须是.hk或者.h2k文件\n并且要和.sip/.zip在同一目录下\n如果是加密压缩包请不要使用本软件\n本软件仅支持rar，zip非加密压缩包'
        BoxLayout:
            size_hint_y: 0.3
            Button:
                font_name:"Han_Font"
                text: "取消"
                on_release: root.dismiss()
            Button:
                font_name:"Han_Font"
                text: "确定"
                on_release:
                    root.parent_inst.selete()
                    root.dismiss()