#:kivy 2.1.0
<WarningPopup@Popup>:
    title: 'Warning'
    size_hint: None, None
    size: 400, 200
    BoxLayout:
        orientation: "vertical"
        Label:
            size_hint: None, None
            size: self.texture_size
            font_name:"Han_Font"
            text: '您确定选择这个文件吗'
        BoxLayout:
            size_hint_y: 0.3
            Button:
                font_name:"Han_Font"
                text: "取消"
                on_release: root.dismiss()
            Button:
                font_name:"Han_Font"
                text: "确定"
                on_release:
                    root.parent_inst.selete()
                    root.dismiss()